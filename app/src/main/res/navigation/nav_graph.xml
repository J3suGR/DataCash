    <fragment
        android:id="@+id/helpFragment"
        android:name="com.example.datacash.ui.profile.HelpFragment"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" />
    <fragment
        android:id="@+id/selectRecipientFragment"
        android:name="com.example.datacash.ui.send.SelectRecipientFragment"
        android:label="fragment_select_recipient"
        tools:layout="@layout/fragment_select_recipient" >
        <action
            android:id="@+id/action_selectRecipientFragment_to_sendMoneyFragment"
            app:destination="@id/sendMoneyFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/sendMoneyFragment"
        android:name="com.example.datacash.ui.send.SendMoneyFragment"
        android:label="fragment_send_money"
        tools:layout="@layout/fragment_send_money" >
        <action
            android:id="@+id/action_sendMoneyFragment_to_sendSuccessFragment"
            app:destination="@id/sendSuccessFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/sendSuccessFragment"
        android:name="com.example.datacash.ui.send.SendSuccessFragment"
        android:label="fragment_send_success"
        tools:layout="@layout/fragment_send_success" >
        <action
            android:id="@+id/action_sendSuccessFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment"
            app:popUpTo="@id/dashboardFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/storeDemoFragment"
        android:name="com.example.datacash.ui.store.StoreDemoFragment"
        android:label="fragment_store_demo"
        tools:layout="@layout/fragment_store_demo" />

    <fragment
        android:id="@+id/serviceDemoFragment"
        android:name="com.example.datacash.ui.services.ServiceDemoFragment"
        android:label="fragment_service_demo"
        tools:layout="@layout/fragment_service_demo" />

</navigation>